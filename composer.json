{
  "name": "veneridze/laravel-pricing-plans",
  "type": "library",
  "description": "A package provide pricing plans for Laravel.",
  "keywords": [
    "plans",
    "laravel",
    "subscriptions",
    "pricing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/veneridze/laravel-pricing-plans",
  "support": {
    "issues": "https://github.com/veneridze/laravel-pricing-plans/issues",
    "source": "https://github.com/veneridze/laravel-pricing-plans"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    },
    {
      "name": "Veneridze",
      "email": "viacheslav.kopaev@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Veneridze\\PricingPlans\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3|^8.0",
    "ext-json": "*",
    "laravel/framework": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "spatie/laravel-package-tools": "^1.18"
  },
  "require-dev": {
    "fakerphp/faker": "^1.13",
    "mockery/mockery": "^1.4.2",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html build/coverage",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": false
  },
  "extra": {
    "laravel": {
      "providers": [
        "Veneridze\\PricingPlans\\PricingPlansServiceProvider"
      ],
      "aliases": {}
    }
  }
}
